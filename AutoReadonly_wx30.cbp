<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="AutoReadonly wx3.0.x" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../devel30/share/CodeBlocks/plugins/AutoReadonly" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../devel30" />
				<Option object_output="../../../.objs30/plugins/contrib/AutoReadonly" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd -p debug" />
				<Option host_application="codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-DHAVE_W32API_H" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DWX_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DcbDEBUG" />
					<Add directory="../../../include" />
					<Add directory="../../../include/tinyxml" />
					<Add directory="../../../sdk/wxscintilla/include" />
					<Add directory="$(#wx30.include)" />
					<Add directory="$(#wx30.lib)/gcc_dll/mswu" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add library="wxmsw30u" />
					<Add directory="../../../devel30" />
					<Add directory="$(#wx30.lib)/gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\devel30\share\CodeBlocks\AutoReadonly.zip manifest.xml *.xrc" />
					<Add after='xcopy /Y images\settings\*.png &quot;..\..\..\devel30\share\Codeblocks\images\settings\*&quot;' />
					<Add after='xcopy /Y images\settings\*.png &quot;..\..\..\output30\share\Codeblocks\images\settings\*&quot;' />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="AutoReadonly.cpp" />
		<Unit filename="AutoReadonly.h" />
		<Unit filename="AutoReadonlyCfg.cpp" />
		<Unit filename="AutoReadonlyCfg.h" />
		<Unit filename="AutoReadonlyCfg.xrc" />
		<Unit filename="DlgAutoReadonlyConfig.cpp" />
		<Unit filename="DlgAutoReadonlyConfig.h" />
		<Unit filename="DlgPathEd.cpp" />
		<Unit filename="DlgPathEd.h" />
		<Unit filename="ReadonlyAbleTool.cpp" />
		<Unit filename="ReadonlyAbleTool.h" />
		<Unit filename="manifest.xml" />
		<Unit filename="wxsmith/DlgPathEd.wxs" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<wxsmith version="1">
				<resources>
					<wxDialog wxs="wxsmith/DlgPathEd.wxs" src="DlgPathEd.cpp" hdr="DlgPathEd.h" fwddecl="0" i18n="1" name="DlgPathEd" language="CPP" />
				</resources>
			</wxsmith>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
